#
# Touchscreen driver configuration
#
config TOUCHSCREEN_ZINITIX_BT404_U8500
	tristate "Zinitix BT404 Touchscreens"
	depends on I2C
	default n
	help
		Say Y if you want support for Zinitix bt404 touchscreen
		controllers.
config TOUCHSCREEN_ZINITIX_U8500
	bool "Zinitix Touchscreens"
	depends on I2C
	default n
	help
		Say Y if you want support for Zinitix  touchscreen
		controllers.
config TOUCHSCREEN_ATMEL_MXT224E_U8500
	bool "Atmel MXT224E Touchscreen Interface"
	depends on I2C
	default n
	help
		Say Y here if you want support for atmel_224E touchscreen controllers.
		To compile this driver as a module, choose M here: the
		module will be called atmel_mxt224E.
